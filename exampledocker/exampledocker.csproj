    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>5dbda4c5-2b74-43bf-904e-f7d44ca9a51f</UserSecretsId>
    <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
